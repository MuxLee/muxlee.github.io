{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "muxlee.github.io": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "outputPath": "dist/muxlee.github.io",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            }
                        ],
                        "styles": [
                            "src/styles.css",
                            {
                                "bundleName": "markdown",
                                "inject": false,
                                "input": "src/markdown.css"
                            }
                        ],
                        "scripts": [
                            {
                                "bundleName": "katex",
                                "inject": false,
                                "input": "node_modules/katex/dist/contrib/auto-render.min.js"
                            },
                            {
                                "bundleName": "katex",
                                "inject": false,
                                "input": "node_modules/katex/dist/katex.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/prism.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-css.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-java.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-javascript.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-json.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-latex.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-markdown.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-mermaid.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-typescript.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-vim.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-yaml.min.js"
                            }
                        ],
                        "prerender": false,
                        "ssr": false
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.development.ts"
                                }
                            ]
                        },
                        "global": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.global.ts"
                                }
                            ]
                        },
                        "local": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.local.ts"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "local"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "muxlee.github.io:build:production"
                        },
                        "development": {
                            "buildTarget": "muxlee.github.io:build:development"
                        },
                        "global": {
                            "buildTarget": "muxlee.github.io:build:global"
                        },
                        "local": {
                            "buildTarget": "muxlee.github.io:build:local"
                        }
                    },
                    "defaultConfiguration": "local"
                },
                "extract-i18n": {
                    "builder": "@angular/build:extract-i18n"
                },
                "test": {
                    "builder": "@angular/build:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            },
                            {
                                "glob": "**/*",
                                "input": "content"
                            }
                        ],
                        "styles": [
                            "src/styles.css",
                            {
                                "bundleName": "markdown",
                                "inject": false,
                                "input": "src/markdown.css"
                            }
                        ],
                        "scripts": [
                            {
                                "bundleName": "katex",
                                "inject": false,
                                "input": "node_modules/katex/dist/contrib/auto-render.min.js"
                            },
                            {
                                "bundleName": "katex",
                                "inject": false,
                                "input": "node_modules/katex/dist/katex.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/prism.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-css.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-java.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-javascript.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-json.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-latex.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-markdown.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-mermaid.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-typescript.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-vim.min.js"
                            },
                            {
                                "bundleName": "prism",
                                "inject": false,
                                "input": "node_modules/prismjs/components/prism-yaml.min.js"
                            }
                        ]
                    }
                }
            }
        }
    },
    "schematics": {
      "@schematics/angular:component": {
        "type": "component"
      },
      "@schematics/angular:directive": {
        "type": "directive"
      },
      "@schematics/angular:service": {
        "type": "service"
      },
      "@schematics/angular:guard": {
        "typeSeparator": "."
      },
      "@schematics/angular:interceptor": {
        "typeSeparator": "."
      },
      "@schematics/angular:module": {
        "typeSeparator": "."
      },
      "@schematics/angular:pipe": {
        "typeSeparator": "."
      },
      "@schematics/angular:resolver": {
        "typeSeparator": "."
      }
    }
}